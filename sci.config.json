{
  "name": "activemq-classic-hawtio",
  "description": "SCI measurement using Impact Framework",
  "pipeline": [
    {
      "name": "measure-build",
      "uses": "@grnsft/if-unofficial-plugins/process-metrics",
      "with": {
        "path": "."
      }
    },
    {
      "name": "sci",
      "uses": "@grnsft/if-plugins/sci",
      "with": {
        "energy": "measure-build.energy",
        "duration": "measure-build.duration",
        "co2e": "measure-build.co2e"
      }
    }
  ]
}
